function Random (z: integer): integer;
var k:integer;
begin
  asm
    la(5) #104
    D1=A
    c=dat1 a
    la(5) #FFF
    c=c&a a
    a=r1
    d1=a
    d1=d1+ #A
    dat1=c a
  end;
  result := k*z div 4096;
end;
