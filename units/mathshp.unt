UNIT MathsHP;

function Abs (x: integer): integer;
begin
  asm
    A=R1
    D1=A
    D1=D1+ 5
    A=DAT1 A
    D1=D1- 5
    LC(5) #7FFFF
    ?C>A A
    GOYES NoNeg
    A=-A A
    :NoNeg
    DAT1=A
  end;
end;

function Abs (x: real): real;
begin
  asm
    A=R1
    D1=A
    D1=D1+ 16
    A=DAT1 W
    D1=D1- 16
    A=0 S
    DAT1=A W
  end;
end;

function Cos (x: real): real;
begin
  asm
    c=r1
    d1=c
    dat0=c a
    d0=d0+ 5
    c=r2
    dat0=c a
    d0=d0+ 5
    c=r3
    dat0=c a
    d0=d0+ 5
    c=r4
    dat0=c a
    cd0ex
    rstk=c
    d1=d1+ 16
    a=dat1 w
    gosbvl #2BC4A	*SPLITA
    HS=0 3
    gosbvl #2aef6	*GETANGMODE
    gosbvl #2b6e9	*COSF 
    gosbvl #29e21	*PACKSB
    P== 0
    SETHEX
    c=rstk
    d0=c
    c=dat0 a
    r4=c
    d0=d0- 5
    c=dat0 a
    r3=c
    d0=d0- 5
    c=dat0 a
    r2=c
    d0=d0- 5
    c=dat0 a
    r1=c
    d1=c
    dat1=a w
  end;
end;

